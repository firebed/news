{
  "name": "firebed/news",
  "description": "News for news agencies using laravel framework",
  "license": "MIT",
  "authors": [
    {
      "name": "okang",
      "email": "okan.giritli@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "intervention/image": "^2.5",
    "paquettg/php-html-parser": "^3.1",
    "firebed/permission": "dev-master",
    "firebed/sitemap": "dev-master"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "laravel/framework": "^8.53"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Firebed\\News\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Firebed\\News\\NewsServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
