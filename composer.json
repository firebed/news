{
  "name": "firebed/news",
  "description": "News for news agencies using laravel framework",
  "license": "MIT",
  "authors": [
    {
      "name": "okang",
      "email": "okan.giritli@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^8.0",
    "livewire/livewire": "^2.3",
    "intervention/image": "^2.5",
    "laravel/fortify": "^1.7",
    "paquettg/php-html-parser": "^3.1",
    "spatie/laravel-cookie-consent": "^2.12"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Firebed\\Components\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Firebed\\News\\NewsServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
